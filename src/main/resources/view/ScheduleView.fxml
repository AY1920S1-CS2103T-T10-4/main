<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<HBox>
    <VBox fx:id="scheduleContainer" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" HBox.hgrow="ALWAYS">
        <Label fx:id="title" HBox.hgrow="ALWAYS" alignment="CENTER" VBox.vgrow="NEVER"/>
        <ScrollPane fx:id="scheduleHeaderWrapper" HBox.hgrow="ALWAYS" fitToWidth="true"/>
        <ScrollPane fx:id="scheduleContents" id="scheduleContents" HBox.hgrow="ALWAYS" fitToWidth="true"/>
    </VBox>
</HBox>
